@using WebApplication.Models
@model UserData
@{
    ViewBag.Title = "About";
}
<h3>Данные из файла</h3>
<p>Серия паспорта: @Model.PassSeries</p>
<p>Номер паспорта: @Model.PassNumber</p>
<p>ФИО: @Model.PassFullName</p>
<p>Дата выдачи паспорта: @Model.PassReceivingDate</p>
<p>Кем выдан паспорт: @Model.PassIss</p>
<p>Адрес прописки: @Model.PassAddress</p>
<p>Серия ВУ: @Model.LicenseSeries</p>
<p>Номер ВУ: @Model.LicenseNumber</p>
<p>Дата выдачи ВУ: @Model.LicenseReceivingDate</p>
<table class="table">
    <thead>
        <tr>
            <th>Номер ТС</th>
            <th>Марка ТС</th>
            <th>Длительность</th>
            <th>Стоимость</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var str in Model.CarList)
        {
            <tr>
                @foreach (var line in str)
                {
                    <td>@line</td>
                }
            </tr>
        }
    </tbody>
</table>